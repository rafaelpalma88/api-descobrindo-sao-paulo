// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Events {
  id          String    @id @default(uuid())
  active      Boolean
  image       String
  title       String
  address     String
  description String
  startDate   String
  endDate     String
  latitude    String
  longitude   String
  created_at  DateTime? @default(now())
  updated_at  DateTime? @default(now())

  @@map("events")
}

model User {
  id         String    @id @default(uuid())
  name       String
  email      String
  password   String
  created_at DateTime? @default(now())
  updated_at DateTime? @default(now())

  @@map("users")
}